eigensolver
{
  "number of eigenvectors" 2
  tolerance 1e-14
}

smoother
{
  type  "Gauss-Seidel"
}

"is preconditioner" false

agglomeration
{
  partitioner block
  nx 2
  ny 2
  nz 2
}

material_property
{
  type constant
}

laplace
{
  distort_random false
  reordering None
  n_refinements 5
}

